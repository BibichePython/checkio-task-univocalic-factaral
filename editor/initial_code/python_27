def factaral(num):
    if num==0: return 1
    return num*factaral(num-1)

if __name__ == '__main__':
    assert factaral(0) == 1
    assert factaral(1) == 1
    assert factaral(2) == 2
    assert factaral(3) == 6
    assert factaral(100) == 93326215443944152681699238856266700490715968264381621468592963895217599993229915608941463976156518286253697920827223758251185210916864000000000000000000000000
